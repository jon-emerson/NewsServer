{namespace news.setuserurlrating autoescape="strict"}

/**
 * Renders the URL rating page.
 */
{template .main}
  <h1>Rank a URL</h1>

  <form method="POST" action="setUserUrlRating">
    Session key:<br>
    <input type="text" name="sessionKey"><br>
    <br>
    URL ID:<br>
    <input type="text" name="urlId"><br>
    <br>
    Rating (double between 0 and 1 inclusive):<br>
    <input type="number" name="rating"><br>
    <br>
    <input type="submit" value="Set"><br>
  </form>
{/template}
