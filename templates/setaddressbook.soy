{namespace news.setaddressbook autoescape="strict"}

/**
 * Renders the connect address book data page.
 */
{template .main}
  <h1>Set Address Book Data</h1>

  <form method="POST" action="/v1/set_address_book">
    Session key:<br>
    <input type="text" name="session_key"><br>
    <br>
    Address Book JSON:<br>
    <textarea rows="4" cols="50" name="data"></textarea><br>
    <br>
    <input type="submit" value="Set"><br>
  </form>
{/template}
