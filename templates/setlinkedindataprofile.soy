{namespace news.setlinkedinprofile autoescape="strict"}

/**
 * Renders the connect linked in data page.
 */
{template .main}
  <h1>Set LinkedIn Data</h1>

  <form method="POST" action="/v1/set_linked_in_profile">
    Session key:<br>
    <input type="text" name="session_key"><br>
    <br>
    LinkedIn JSON:<br>
    <textarea rows="4" cols="50" name="data"></textarea><br>
    <br>
    <input type="submit" value="Set"><br>
  </form>
{/template}
