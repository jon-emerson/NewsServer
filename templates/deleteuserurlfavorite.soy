{namespace news.deleteuserurlfavorite autoescape="strict"}

/**
 * Renders the delete favorite URL page.
 */
{template .main}
  <h1>Delete a favorite URL</h1>

  <form method="POST" action="deleteUserUrlFavorite">
    Session key:<br>
    <input type="text" name="sessionKey"><br>
    <br>
    URL ID:<br>
    <input type="text" name="urlId"><br>
    <br>
    <input type="submit" value="Delete"><br>
  </form>
{/template}
