  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>com.janknspank.server.IndexServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern></url-pattern>
