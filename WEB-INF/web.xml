    <servlet-class>com.janknspank.server.SetAddressBookDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SetAddressBookDataServlet</servlet-name>
    <url-pattern>/setAddressBookData</url-pattern>
