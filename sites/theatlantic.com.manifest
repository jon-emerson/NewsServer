root_domain: "theatlantic.com"
start_url: "http://www.theatlantic.com"
paragraph_selector: ".article-content p"
paragraph_selector: ".article-body p"
article_url_pattern {
  path_regex: "\\/20[0-9]{2}\\/[01][0-9]\\/[^\\/]+\\/[0-9]+\\/$"
}
test_instructions {
